FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 1e-20;

boundaryField
{
    topWall
    {
        type            epsilonWallFunction;
        value           nonuniform List<scalar> 0();
    }
    botWall
    {
        type            epsilonWallFunction;
        value           nonuniform List<scalar> 0();
    }
    inlet
    {
        type            cyclic;
    }
    outlet
    {
        type            cyclic;
    }
    fixWall
    {
        type            empty;
    }
    procBoundary5to4
    {
        type            processor;
        value           uniform 1e-20;
    }
    procBoundary5to6
    {
        type            processor;
        value           uniform 1e-20;
    }
}


// ************************************************************************* //
