FoamFile
{
    format      ascii;
    class       dictionary;
    location    "constant";
    object      transportProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

phases (water air);

water
{
    transportModel  Newtonian;
    nu              1.88679245283e-4;
    rho             10;
}

air
{
    transportModel  Newtonian;
    nu              1.88679245283e-4;
    rho             1;
}

sigma           0.07;


