FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];


internalField   uniform 0.1;

boundaryField
{

    topWall
    {
        type            fixedValue;
        value           uniform 0.0001;
    }

    botWall
    {
        type            fixedValue;
        value           uniform 0.0001;
    }

    inlet
    {
        type            cyclic;
    }

    outlet
    {
        type            cyclic;
    }



    fixWall
    {
        type            empty;
    }


}


